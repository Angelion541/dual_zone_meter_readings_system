{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "sourceType": "module",
    "requireConfigFile": false,
    "ecmaVersion": 2020,
    "babelOptions": {
      "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
      ]
    },
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": [
    // "eslint:recommended",
    // "plugin:testing-library/react",
    // "plugin:react/recommended"
  ],
  "rules": {
    "no-restricted-syntax": "off",
    "react/react-in-jsx-scope": "off"
  },
  "env": {
    "browser": true,
    "es2021": true
  },
  "plugins": [
    "react"
  ]
}